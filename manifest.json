  {
    "manifest_version": 3,
    "name": "YouTube Ad Blocker",
    "version": "1.0",
    "description": " Enjoy the FRZF7 Automatically YouTube AdBlocker.",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
      },
      "icons": {
        "48": "icon.png"
      }
  }
  